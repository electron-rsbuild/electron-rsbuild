    "cleanup": "rimraf node_modules"
  },
  "author": "veaba",
  "dependencies": {
    "@electron-rsbuild/core": "workspace:*",
    "@rsbuild/core": "^1.1.0",
    "@rslib/core": "^0.0.16"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
