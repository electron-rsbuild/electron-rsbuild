  "dependencies": {
    "create-rstack": "1.0.9"
  },
  "devDependencies": {
    "@rslib/core": "0.0.18",
    "@types/node": "18.x",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
