{
  "name": "electron-rsbuild-monorepo",
  "private": true,
  "author": "veaba<godpu@oulook.com>",
  "scripts": {
    "react": "pnpm --filter=electron-rsbuild-react run dev",
    "preview": "pnpm --filter=electron-rsbuild-react run preview",
    "electron": "pnpm --filter=electron-rsbuild-react run build",
    "dev": "nx dev @electron-rsbuild/core",
    "build": "nx build @electron-rsbuild/core",
    "clear": "nx run-many -t clear",
    "dev:packages": "pnpm run dev:plugins && pnpm run dev:core",
    "dev:core": "pnpm --filter=@electron-rsbuild/core run dev",
    "dev:plugins": "pnpm --filter=@electron-rsbuild/plugin-* run dev",
    "build:core": "pnpm --filter=core run build",
    "lint": "biome check . --diagnostic-level=warn",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veaba/electron-rsbuild.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/veaba/electron-rsbuild/issues"
  },
  "homepage": "https://github.com/veaba/electron-rsbuild#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "nx": "^20.0.12",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/compat/*",
      "scripts/*"
    ]
  },
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@rsbuild/core": "^1.1.0"
  }
}