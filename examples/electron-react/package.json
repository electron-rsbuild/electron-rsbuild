{
  "name": "electron-rsbuild-app",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "private": true,
  "type": "commonjs",
  "author": "veaba<godpu@oulook.com>",
  "homepage": "https://electron-rsbuild.org",
  "scripts": {
    "format": "prettier --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "rsbuild:build": "rsbuild build",
    "rsbuild:dev": "rsbuild dev",
    "rebuild:build-main": "rsbuild build -c rsbuild-main.config.ts",
    "rebuild:build-preload": "rsbuild build rsbuild-preload.config.ts",
    "builder": "electron-builder --dir",
    "test": "vitest dev",
    "start": "electron-rsbuild preview",
    "dev": "electron-rsbuild dev",
    "inspect": "bun --inspect ./node_modules/electron-rsbuild/bin/electron-rsbuild.ts dev",
    "build": "npm run typecheck && electron-rsbuild build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-rsbuild build && electron-builder --mac",
    "build:linux": "electron-rsbuild build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@rsbuild/core": "^1.1.0",
    "@rsbuild/plugin-node-polyfill": "^1.2.0",
    "@rsbuild/plugin-react": "^1.0.7"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-rsbuild/core": "workspace:*",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "vitest": "^2.1.4"
  }
}