{
  "name": "@electron-rsbuild/plugin-renderer",
  "version": "0.0.10",
  "description": "Electron main config plugin for Electron-Rsbuild",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "rslib build --watch",
    "start": "rslib build --watch",
